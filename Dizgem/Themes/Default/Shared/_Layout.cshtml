<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    @DizgemHelper.CreateMetaTags(SettingsService, Context.Request, (Model is Post ? Model : null))

    <link href="/assets/docs/5.0/examples/blog/" rel="canonical" />
    <!-- Bootstrap core CSS -->
    <link href="/assets/docs/5.0/dist/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Favicons -->
    <link href="/assets/docs/5.0/assets/img/favicons/apple-touch-icon.png" rel="apple-touch-icon" sizes="180x180" />
    <link href="/assets/docs/5.0/assets/img/favicons/favicon-32x32.png" rel="icon" sizes="32x32" type="image/png" />
    <link href="/assets/docs/5.0/assets/img/favicons/favicon-16x16.png" rel="icon" sizes="16x16" type="image/png" />
    <link href="/assets/docs/5.0/assets/img/favicons/manifest.json" rel="manifest" />
    <link color="#7952b3" href="assets/docs/5.0/assets/img/favicons/safari-pinned-tab.svg" rel="mask-icon" />
    <link href="/assets/docs/5.0/assets/img/favicons/favicon.ico" rel="icon" />
    @SettingsService.Current
    <meta content="#7952b3" name="theme-color" />
    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            user-select: none;
        }

        @@media (min-width: 768px) {
            .bd-placeholder-img-lg {
                font-size: 3.5rem;
            }
        }

        ul.custom-style {
            flex-direction: row !important;
            flex: 1 1 auto !important;
            width: 100% !important;
        }

            ul.custom-style .nav-item {
                margin-right: .5rem !important;
            }
    </style>
    <!-- Custom styles for this template -->
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:700,900&amp;display=swap" rel="stylesheet" />
    <!-- Custom styles for this template -->
    <link href="/assets/docs/5.0/examples/blog/blog.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        <header class="blog-header py-3">
            <div class="row flex-nowrap justify-content-between align-items-center">
                <div class="col-4 pt-1">
                    <a class="link-secondary" href="@Url.Action("Index", "Home")">Subscribe</a>
                </div>
                <div class="col-4 text-center">
                    <a class="blog-header-logo text-dark" href="@Url.Action("Index", "Home")">Large</a>
                </div>
                <div class="col-4 d-flex justify-content-end align-items-center">
                    <a aria-label="Search" class="link-secondary" href="@Url.Action("Index", "Home")">
                        <svg class="mx-3"
                             fill="none"
                             height="20"
                             role="img"
                             stroke="currentColor"
                             stroke-linecap="round"
                             stroke-linejoin="round"
                             stroke-width="2"
                             viewbox="0 0 24 24"
                             width="20"
                             xmlns="http://www.w3.org/2000/svg">
                            <title>Search</title>
                            <circle cx="10.5" cy="10.5" r="7.5"></circle>
                            <path d="M21 21l-5.2-5.2"></path>
                        </svg>
                    </a>
                    <a class="btn btn-sm btn-outline-secondary" href="@Url.Action("Index", "Home")">Sign up</a>
                </div>
            </div>
        </header>
        <div class="nav-scroller py-1 mb-2">
            <nav class="navbar navbar-nav">
                @await MenuService.RenderMenuAsync("primary_navigation")
            </nav>

            @* <nav class="nav d-flex justify-content-between">

                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">World</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">U.S.</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Technology</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Design</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Culture</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Business</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Politics</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Opinion</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Science</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Health</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Style</a>
                <a class="p-2 link-secondary" href="@Url.Action("Index", "Home")">Travel</a>
            </nav> *@
        </div>
    </div>
    <main class="main pt-4">
        @RenderBody()
    </main>
    <footer class="blog-footer">
        <p>
            Blog template built for <a href="@Url.Action("Index", "Home")">Bootstrap</a> by
            <a href="https://twitter.com/mdo">@@mdo</a>.
        </p>
        <p>
            <a href="@Url.Action("Index", "Home")">Back to top</a>
        </p>
    </footer>
</body>
</html>
